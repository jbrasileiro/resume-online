<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.SeparatorMenuItem?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<fx:root
	xmlns:fx="http://javafx.com/fxml"
	prefHeight="143.0"
	prefWidth="640.0"
	type="VBox"
	fx:controller="resumeonline.jfx.controller.ApplicationContoller"
>
	<children>
		<MenuBar VBox.vgrow="NEVER">
			<menus>
				<Menu
					mnemonicParsing="false"
					text="%file"
				>
					<items>
						<MenuItem
							mnemonicParsing="false"
							text="%new" />
						<MenuItem
							mnemonicParsing="false"
							text="%open.ellipsis" />
						<Menu
							mnemonicParsing="false"
							text="%open.recent" />
						<SeparatorMenuItem mnemonicParsing="false" />
						<MenuItem
							mnemonicParsing="false"
							text="%close" />
						<MenuItem
							mnemonicParsing="false"
							text="%save" />
						<MenuItem
							mnemonicParsing="false"
							text="%save.as.ellipsis" />
						<MenuItem
							mnemonicParsing="false"
							text="%revert" />
						<SeparatorMenuItem mnemonicParsing="false" />
						<MenuItem
							mnemonicParsing="false"
							text="%preferences.ellipsis" />
						<SeparatorMenuItem mnemonicParsing="false" />
						<MenuItem
							mnemonicParsing="false"
							text="%quit" />
					</items>
				</Menu>
				<Menu
					mnemonicParsing="false"
					text="%edit"
				>
					<items>
						<MenuItem
							mnemonicParsing="false"
							text="%undo" />
						<MenuItem
							mnemonicParsing="false"
							text="%redo" />
						<SeparatorMenuItem mnemonicParsing="false" />
						<MenuItem
							mnemonicParsing="false"
							text="%cut" />
						<MenuItem
							mnemonicParsing="false"
							text="%copy" />
						<MenuItem
							mnemonicParsing="false"
							text="%paste" />
						<MenuItem
							mnemonicParsing="false"
							text="%delete" />
						<SeparatorMenuItem mnemonicParsing="false" />
						<MenuItem
							mnemonicParsing="false"
							text="%select.all" />
						<MenuItem
							mnemonicParsing="false"
							text="%unselect.all" />
					</items>
				</Menu>
				<Menu
					mnemonicParsing="false"
					text="%help"
				>
					<items>
						<MenuItem
							mnemonicParsing="false"
							text="%about" />
					</items>
				</Menu>
			</menus>
		</MenuBar>
		<AnchorPane
			fx:id="pane"
			styleClass="default-box"
			VBox.vgrow="ALWAYS"
		>
			<children>
				<VBox
					styleClass="box-border, default-padding"
					AnchorPane.leftAnchor="0.0"
					AnchorPane.rightAnchor="0.0"
					AnchorPane.topAnchor="0.0"
				>
					<children>
						<HBox
							alignment="CENTER"
							prefHeight="25.0"
							prefWidth="429.0"
							VBox.vgrow="ALWAYS"
						>
							<Label
								text="%directory.colon"
								HBox.hgrow="ALWAYS"
							>
								<styleClass>
									<String fx:value="default-border" />
								</styleClass>
							</Label>
							<Label
								fx:id="lblDirectory"
								prefHeight="23.0"
								prefWidth="412.0"
								styleClass="default-border"
								HBox.hgrow="ALWAYS" />
							<Button
								mnemonicParsing="false"
								onAction="#onActionDirectoryChooser"
								styleClass="directory-button"
							>
								<graphic>
									<ImageView
										pickOnBounds="false"
										preserveRatio="false"
										styleClass="directory-image-view" />
								</graphic>
							</Button>
							<Button
								mnemonicParsing="false"
								onAction="#onActionExecute"
								text="%open" />
						</HBox>
					</children>
				</VBox>
			</children>
			<VBox.margin>
				<Insets />
			</VBox.margin>
		</AnchorPane>
	</children>
</fx:root>